
  <%= simple_form_for [venue, booking] do |f| %>
   <div>
      <%= f.input :booking_date,
      as: :string,
      input_html: { data: { controller: "datepicker" } }, class: "input-bf" %>
    </div>

    <div class="button-container-bf ">
      <%= f.submit "Reserve now", class: "btn-booking-form" %>
    </div>
  <% end %>
